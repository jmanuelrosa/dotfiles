# Example secrets file - Copy values to secrets.yml and encrypt with ansible-vault
#
# To decrypt and edit: ansible-vault edit vars/secrets.yml
# To encrypt: ansible-vault encrypt vars/secrets.yml

# NPM Token for private package access
DID_NPM_TOKEN: "your-npm-token-here"

# Jira credentials for s-task script
DID_JIRA_EMAIL: "your-email@example.com"
DID_JIRA_API_TOKEN: "your-jira-api-token-here"
